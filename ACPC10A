#include<bits/stdc++.h>
using namespace std;
int main()
{
    int x,y,z;
    while (1)
    {
        cin>>x>>y>>z;
        if (x==0 && y==0 && z==0)
        break;
        if (y-x==z-y)
        {
            cout<<"AP "<<(z+(y-x))<<endl;
        }
        else
        {
            cout<<"GP "<<(z*(y/x))<<endl;
        }
    }
        return 0;
}